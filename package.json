{
  "name": "prettier-plugin-tailwindcss-eslint-conflict",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "bootstrap": "corepack enable && yarn --immutable",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettier:watch-write": "chokidar 'src/**/*.{ts,tsx}' -c \"test '{event}' = 'change' && (prettier --write '{path}')\"",
    "prettier:check": "prettier --check ./src/**/*",
    "eslint:fix-dry-run": "eslint --fix-dry-run --ext .ts,.tsx ./src",
    "lint": "next lint"
  },
  "dependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "chokidar-cli": "^3.0.0",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "tailwindcss": "^3.4.3",
    "typescript": "^5"
  }
}
